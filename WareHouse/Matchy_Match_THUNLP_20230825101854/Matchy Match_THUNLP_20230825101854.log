        self.value = value
        self.button = None
        self.is_visible = False
    def __str__(self):
        return str(self.value)
    def show(self):
        # Show the card value
        self.button.config(text=str(self))
        self.is_visible = True
    def hide(self):
        # Hide the card value
        self.button.config(text=" ")
        self.is_visible = False
