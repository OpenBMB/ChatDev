      <h2>Active Todo Items</h2>
      <TodoList todos={activeTodos} handleCompleteTodo={handleCompleteTodo} />
      <h2>Completed Todo Items</h2>
      <TodoList todos={completedTodos} />
    </div>
  );
}
export default App;
