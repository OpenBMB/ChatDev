        self.zoom *= 2.0
        self.redraw_pixels()

    def zoom_out(self):
        self.zoom /= 2.0
        self.redraw_pixels()

    def redraw_pixels(self):
        self.delete("all")
        for (x, y), color in self.pixels.items():
            self.draw_pixel(x, y)

