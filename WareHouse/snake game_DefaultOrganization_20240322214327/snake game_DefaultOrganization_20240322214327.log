            self.dy = -self.size
        elif keys[pygame.K_DOWN] and self.dy != -self.size:
            self.dx = 0
            self.dy = self.size
        self.x += self.dx
        self.y += self.dy
        # Update the snake's body
        self.body.insert(0, (self.x, self.y))
        if len(self.body) > 1:
            self.body.pop()
