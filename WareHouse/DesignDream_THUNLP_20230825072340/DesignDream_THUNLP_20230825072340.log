        self.room.add_furniture("chair")
        self.update_canvas()

    def add_table(self):
        # Add a table to the room
        self.room.add_furniture("table")
        self.update_canvas()

    def update_canvas(self):
        # Clear canvas and redraw room and furniture
        self.canvas.delete("all")
        self.room.draw(self.canvas)

