        total_expenses = self.calculate_total_expenses()
        net_income = total_income - total_expenses
        report = f"Total Income: ${total_income}\n"
        report += f"Total Expenses: ${total_expenses}\n"
        report += f"Net Income: ${net_income}\n"
        return report
