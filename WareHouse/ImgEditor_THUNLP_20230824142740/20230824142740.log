                self.current_image = self.current_image.resize((new_width, new_height), Image.ANTIALIAS)
            self.image_tk = ImageTk.PhotoImage(self.current_image)
            self.canvas.create_image(0, 0, anchor=tk.NW, image=self.image_tk)
